{
  "version": 2,
  "name": "bistro-bert",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "regions": ["bru1"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_ENV": "production",
    "NODE_ENV": "production",
    "SKIP_CANVAS_INSTALL": "true",
    "PDFJS_DIST_BUILD_SCRIPT": "false"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_ENV": "production",
      "NODE_ENV": "production",
      "SKIP_CANVAS_INSTALL": "true",
      "PDFJS_DIST_BUILD_SCRIPT": "false"
    }
  },
  "git": {
    "deploymentEnabled": {
      "master": true,
      "staging": true
    }
  }
}